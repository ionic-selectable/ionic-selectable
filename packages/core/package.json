{
  "name": "@ionic-selectable/core",
  "title": "Ionic Selectable",
  "version": "8.0.0-alpha.5",
  "description": "An Ionic component similar to Ionic Select, that allows to search items, including async search, group, add, edit, delete items, and much more.",
  "author": {
    "name": "Evgenii Koriakin",
    "url": "https://github.com/ionic-selectable"
  },
  "license": "MIT",
  "homepage": "https://github.com/ionic-selectable/ionic-selectable",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-selectable/ionic-selectable.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-selectable/ionic-selectable/issues"
  },
  "keywords": [
    "ionic",
    "ionic 2",
    "ionic 3",
    "ionic 4",
    "ionic 5",
    "ion-select",
    "search",
    "searchable",
    "searchbar",
    "select",
    "selectable"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ionic-selectable/ionic-selectable.esm.js",
  "exports": {
    ".": {
      "import": "./dist/ionic-selectable/ionic-selectable.esm.js",
      "require": "./dist/ionic-selectable/ionic-selectable.cjs.js"
    },
    "./ionic-selectable": {
      "import": "./dist/components/ionic-selectable.js",
      "types": "./dist/components/ionic-selectable.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@ionic/core": "^8.4.0"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.0",
    "@stencil/sass": "^3.0.12",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "peerDependencies": {
    "@ionic/core": "8.x"
  },
  "publishConfig": {
    "directory": "/",
    "access": "public"
  }
}
